:root {
    --muse-chat-backgrounds-50: #aaaaaa;
    --muse-chat-backgrounds-75: #a9a9a9;
    --muse-chat-backgrounds-100: #c0c0c0;
    --muse-chat-backgrounds-150: #c8c8c8;
    --muse-chat-backgrounds-175: #e3e3e3;
    --muse-chat-backgrounds-200: #cbcbcb;
    --muse-chat-backgrounds-250: #cacaca;
    --muse-chat-foregrounds-300: #cccccc;
    --muse-chat-foregrounds-310: #cccccc;
    --muse-chat-foregrounds-320: #bdbdbd;
    --muse-chat-backgrounds-350: #e3e3e3;
    --muse-chat-foregrounds-25: #ececec;
    --muse-chat-text-primary: #090909;
    --muse-chat-text-primary-inverted: #d2d2d2;
    --muse-chat-text-secondary: rgba(20, 20, 20, 0.5);
    --muse-chat-text-thoughts: rgba(20, 20, 20, 0.65);
    --muse-chat-text-answers: rgba(20, 20, 20, 1);
    --muse-chat-conversation-dragger: #9A9A9A;
    --muse-chat-conversation-dragger-hover: #8E8E8E;
    --muse-chat-text-secondary-on-background-50: rgba(29, 29, 29, 1);
    --muse-chat-text-arguments: #886726;
    --muse-chat-primary-500: #868686;
    --muse-chat-accent-50: #3a72b0;
    --muse-chat-accent-100: #4c7eff;
    --muse-chat-button-disabled: rgba(228, 228, 228, 0.3);
    --muse-chat-button-pressed: #96c3fb;
    --muse-chat-icon-missing: #d33c79;
    --muse-chat-icon-success-color: #4CAF50;
    --muse-chat-icon-warning-color: #E6B00C;
    --muse-chat-icon-error-color: #FF534A;
    --muse-chat-warning-color: #ffce59;
    --muse-chat-warning-background-color: rgba(255, 206, 89, 0.3);
    --muse-chat-button-active: #96C3FB;
    --muse-chat-prefab-color: #018cff;
    --muse-chat-code-mark-error: rgba(255, 81, 81, 0.3);
    --muse-chat-code-mark-info: rgba(76, 76, 255, 0.3);
    --muse-chat-image-hover-tint: #090909;
    --muse-chat-separator-line: rgba(0,0,0,0.1);

    /* Specific to this mode only */
    --muse-chat-light-mode-icon-tint: #383838;
    --muse-chat-light-mode-icon-tick-tint: #008126;

    /* Legacy Colors To Remove */
    --muse-chat-color-invalid: #dc00ff;

    /* Specialized overrides for dark + light discrepancy */
    --muse-chat-popup-hover: #d6d6d6;
    --muse-chat-popup-tab-hover: #222222;
}

.-muse-chat-panel {
    background-color: var(--muse-chat-backgrounds-200);
}

.muse-chat-icon {
    ---unity-image-tint-color: blue;
}

.mui-icon-empty {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/empty.png');
}

.mui-icon-empty-margin {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/empty.png');
    width: 28px;
    height: 14px;
}

.mui-icon-history {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/UndoHistory.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-copy {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Copy.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-save {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/SaveAs.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-edit {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Code.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-delete {
    background-image: resource('d_console.erroricon');
}

.mui-icon-upvote {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/ThumbsUp.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-downvote {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/ThumbsDown.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-tick {
    --unity-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/CompletedTask.png');
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tick-tint);
}

.mui-icon-star-filled-white {
    --unity-image: url("project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Favorites.png");
    --unity-image-tint-color: var(--muse-chat-text-primary);
}

.mui-icon-star-filled-grey {
    --unity-image: url("project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Favorites.png");
    --unity-image-tint-color: var(--muse-chat-text-secondary);
}

.mui-icon-star-filled-grey:hover {
    --unity-image-tint-color: var(--muse-chat-image-hover-tint);
}

.mui-icon-close-s-trimmed {
    --unity-image: url("project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Close_S_Trimmed.png");
}

.mui-icon-close {
    --unity-image: url("project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/Close.png");
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-arrow-square-in {
    --unity-image: url("project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/ArrowSquareIn.png");
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
}

.mui-icon-undo {
    --unity-image:resource("Icons/d_UndoHistory.png");
    --unity-image-tint-color: var(--muse-chat-light-mode-icon-tint);
