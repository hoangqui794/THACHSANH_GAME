/* Animation frames for the progress icon using Unity logo cycle */
.mui-icon-progress-frame-0 {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/UnityLogoCylce_Light_1.png');
}

.mui-icon-progress-frame-1 {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/UnityLogoCylce_Light_2.png');
}

.mui-icon-progress-frame-2 {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/UnityLogoCylce_Light_3.png');
}

.mui-icon-progress-frame-3 {
    background-image: url('project://database/Packages/com.unity.ai.assistant/Editor/UI/Assets/icons/UnityLogoCylce_Light_4.png');
